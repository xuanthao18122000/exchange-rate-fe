        "@types/lodash": "^4.14.187",
        "@vee-validate/i18n": "^4.7.2",
        "@vee-validate/rules": "^4.7.2",
        "axios": "^1.1.3",
        "bootstrap": "^5.2.2",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "perfect-scrollbar": "^1.5.5",
        "pinia": "^2.0.23",
        "pinia-plugin-persistedstate": "^2.4.0",
        "sass": "^1.55.0",
        "vee-validate": "^4.7.2",
        "vue": "^3.2.41",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.1.5",
        "vue-toastification": "^2.0.0-rc.5"
    },
    "devDependencies": {
