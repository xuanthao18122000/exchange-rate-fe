        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.1",
        "npm-run-all": "^4.1.5",
        "typescript": "~4.7.4",
        "vite": "^3.1.8",
        "vitest": "^0.24.3",
        "vue-tsc": "^1.0.9"
    }
